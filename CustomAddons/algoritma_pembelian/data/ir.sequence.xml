<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="algoritma_pembelian_seq" model="ir.sequence">
        <field name="name">algortima.pembelian.seq</field>
        <field name="code">algoritma.pembelian</field>
        <field name="prefix">AP%(year)s/%(month)s/</field>
        <field eval="1" name="number_next"></field>
        <field eval="1" name="number_increment"></field>
        <field eval="True" name="use_date_range"></field>
        <field name="company_id" eval="False"></field>
        <field name="padding">6</field>
    </record>
</odoo>